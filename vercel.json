{
  "version": 2,
  "name" : "Foodkart",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "functions": {
          "routes/*.js": { "maxDuration": 60 },
          "middleware/*.js": { "maxDuration": 60 }
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
